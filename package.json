{
  "name": "server",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prettier": "prettier --check src",
    "prettify": "prettier --write src",
    "dev": "nodemon -e js,json,ts --watch src --exec babel-node ./src/bin/www.js --extensions .ts,.js",
    "clean": "rimraf dist",
    "prebuild": "yarn run clean",
    "build": "babel src -d dist --extensions .ts,.js",
    "prestart": "yarn run build",
    "start": "node dist/bin/www.js",
    "types": "ts-node ./script/types.ts",
    "yaml": "ts-node-dev downloadAPI.ts"
  },
  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "@types/node-fetch": "^2.5.12",
    "@types/yamljs": "^0.2.31",
    "babel-plugin-add-module-exports": "^1.0.4",
    "bson": "^4.4.1",
    "chalk": "^4.1.1",
    "colors": "^1.4.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-openapi-validator": "^4.12.15",
    "http-errors": "~1.6.3",
    "jest": "^27.0.6",
    "joi": "^17.3.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.13.9",
    "mongoose-type-url": "^2.1.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "pug": "~3.0.0",
    "swagger-routes-express": "^3.3.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.11",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/express": "^4.17.12",
    "@types/history": "^4.7.3",
    "@types/jest": "^26.0.24",
    "@types/mongodb": "^3.6.19",
    "@types/node": "^14.14.33",
    "joi-to-typescript": "^2.1.0",
    "mongoose-tsgen": "^8.3.5",
    "node-fetch": "^2.6.1",
    "openapi-typescript": "^4.0.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "write-yaml-file": "^4.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "babel-plugin-add-module-exports",
      "@babel/plugin-transform-typescript",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
